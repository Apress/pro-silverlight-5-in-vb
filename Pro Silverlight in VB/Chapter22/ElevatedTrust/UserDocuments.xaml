<UserControl x:Class="ElevatedTrust.UserDocuments"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <StackPanel>
        <Button x:Name="cmdWrite" Click="cmdWrite_Click">Write File</Button>
        <Button x:Name="cmdRead" Click="cmdRead_Click">Read File</Button>
        <TextBlock x:Name="lblResults" TextWrapping="Wrap"></TextBlock>
    </StackPanel>
</UserControl>
